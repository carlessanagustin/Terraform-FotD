##############################################
# Function: cidrnetmask
##############################################
##############################################
# Variables
##############################################
variable "iprange" {
  default = "10.0.0.0/16"
}

##############################################
# Resources
##############################################

##############################################
# Outputs
##############################################
output "1_iprange" {
  value = "${var.iprange}"
}

output "2_cidrnetmask_output" {
  value = "${cidrnetmask(var.iprange)}"
}
