##############################################
# Function: md5
##############################################
##############################################
# Variables
##############################################
variable "md5" {}

variable "fileText" {
  default = "textFile.txt"
}

##############################################
# Resources
##############################################
##############################################
# Outputs
##############################################
output "md5_output" {
  value = "${md5(var.md5)}"
}

output "md5_file" {
  value = "${md5(file(var.fileText))}"
}

